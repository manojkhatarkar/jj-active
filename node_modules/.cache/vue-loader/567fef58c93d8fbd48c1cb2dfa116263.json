eedback', data)\r\n                    if (data.status == 'success') {\r\n                        self.$toasted.global.success({\r\n                            message: 'Thankyou for feedback'\r\n                        })\r\n                        self.userRating = data.data\r\n                        self.feedbackDatas()\r\n                    } else if (data.status == 'error') {\r\n                        // self.userRating = data.data\r\n                    }\r\n                });\r\n            }\r\n\r\n        },\r\n        feedbackDatas() {\r\n            var self = this;\r\n            axios({\r\n                method: \"POST\",\r\n                url: this.$root.URL_ROOT + \"api.php\",\r\n                data: {\r\n                    type: \"feedbackData\",\r\n                    pid: self.$route.params.id,\r\n                },\r\n            }).then(function (response) {\r\n                var data = response.data;\r\n                if (data.status == 'success') {\r\n                    // console.log('feedbackData', data.data)\r\n                    self.feedbackData = data.data\r\n                } else {\r\n                    self.feedbackData = false\r\n                }\r\n            });\r\n\r\n        },\r\n    },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.max-50 {\r\n    max-width: 50px;\r\n    max-height: 120px;\r\n}\r\n\r\n.detail-iframe {\r\n    width: 355px;\r\n}\r\n\r\n.pb-10 {\r\n    padding-bottom: 10px;\r\n}\r\n\r\n.main-img {\r\n    max-width: 113px;\r\n    margin-right: 8px !important;\r\n}\r\n\r\n.click-disabled {\r\n    pointer-events: none;\r\n    cursor: default;\r\n}\r\n\r\n.swiper {\r\n    width: 100%;\r\n    height: 100%;\r\n}\r\n\r\n.swiper-slide {\r\n    text-align: center;\r\n    font-size: 18px;\r\n    background: #fff;\r\n\r\n    /* Center slide text vertically */\r\n    display: -webkit-box;\r\n    display: -ms-flexbox;\r\n    display: -webkit-flex;\r\n    display: flex;\r\n    -webkit-box-pack: center;\r\n    -ms-fl